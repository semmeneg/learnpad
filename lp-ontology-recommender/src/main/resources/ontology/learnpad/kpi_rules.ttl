<http://ikm-group.ch/archiMEO/kpi#KPI>
  spin:rule [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object [
                sp:varName "kpi" ;
              ] ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "valueInstance" ;
              ] ;
          ]
          [
            sp:object <http://learnpad.eu#PerformanceValue> ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "valueInstance" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "minScore" ;
              ] ;
            sp:predicate <http://learnpad.eu#performanceValueHasActualValue> ;
            sp:subject [
                sp:varName "valueInstance" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "organisationalUnit" ;
              ] ;
            sp:predicate <http://learnpad.eu#performanceValueBelongsToBusinessActor> ;
            sp:subject [
                sp:varName "valueInstance" ;
              ] ;
          ]
        ) ;
      sp:where (
          [
            rdf:type sp:SubQuery ;
            sp:query [
                rdf:type sp:Select ;
                sp:groupBy (
                    [
                      sp:varName "organisationalUnit" ;
                    ]
                    [
                      sp:varName "kpi" ;
                    ]
                  ) ;
                sp:resultVariables (
                    [
                      sp:varName "organisationalUnit" ;
                    ]
                    [
                      sp:varName "kpi" ;
                    ]
                    [
                      sp:expression [
                          rdf:type sp:Min ;
                          sp:expression [
                              sp:varName "score" ;
                            ] ;
                        ] ;
                      sp:varName "minScore" ;
                    ]
                  ) ;
                sp:where (
                    [
                      sp:object [
                          sp:varName "score" ;
                        ] ;
                      sp:predicate <http://learnpad.eu#performanceValueHasScore> ;
                      sp:subject [
                          sp:varName "kpiValue" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "businessActor" ;
                        ] ;
                      sp:predicate <http://learnpad.eu#performanceValueBelongsToBusinessActor> ;
                      sp:subject [
                          sp:varName "kpiValue" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "kpi" ;
                        ] ;
                      sp:predicate rdf:type ;
                      sp:subject [
                          sp:varName "kpiValue" ;
                        ] ;
                    ]
                    [
                      sp:object <http://ikm-group.ch/archiMEO/kpi#KPI> ;
                      sp:predicate rdf:type ;
                      sp:subject [
                          sp:varName "kpi" ;
                        ] ;
                    ]
                    [
                      rdf:type sp:Optional ;
                      sp:elements (
                          [
                            sp:object [
                                sp:varName "role" ;
                              ] ;
                            sp:predicate <http://ikm-group.ch/archiMEO/omm#performerHasRole> ;
                            sp:subject [
                                sp:varName "businessActor" ;
                              ] ;
                          ]
                          [
                            sp:object [
                                sp:varName "organisationalUnit" ;
                              ] ;
                            sp:predicate <http://learnpad.eu#roleIsCastedByOrgUnit> ;
                            sp:subject [
                                sp:varName "role" ;
                              ] ;
                          ]
                          [
                            sp:object [
                                sp:varName "organisationalUnitName" ;
                              ] ;
                            sp:predicate rdfs:label ;
                            sp:subject [
                                sp:varName "organisationalUnit" ;
                              ] ;
                          ]
                        ) ;
                    ]
                    [
                      rdf:type sp:Optional ;
                      sp:elements (
                          [
                            sp:object [
                                sp:varName "businessActor" ;
                              ] ;
                            sp:predicate <http://ikm-group.ch/archiMEO/emo#performerRepresentsPerson> ;
                            sp:subject [
                                sp:varName "performer" ;
                              ] ;
                          ]
                          [
                            sp:object [
                                sp:varName "role" ;
                              ] ;
                            sp:predicate <http://ikm-group.ch/archiMEO/omm#performerHasRole> ;
                            sp:subject [
                                sp:varName "performer" ;
                              ] ;
                          ]
                          [
                            sp:object [
                                sp:varName "organisationalUnit" ;
                              ] ;
                            sp:predicate <http://learnpad.eu#roleIsCastedByOrgUnit> ;
                            sp:subject [
                                sp:varName "role" ;
                              ] ;
                          ]
                        ) ;
                    ]
                    [
                      rdf:type sp:Filter ;
                      sp:expression [
                          rdf:type sp:bound ;
                          sp:arg1 [
                              sp:varName "organisationalUnit" ;
                            ] ;
                        ] ;
                    ]
                  ) ;
              ] ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type sp:iri ;
                sp:arg1 [
                    rdf:type fn:concat ;
                    sp:arg1 "http://learnpad.eu/exec#" ;
                    sp:arg2 [
                        rdf:type afn:localname ;
                        sp:arg1 [
                            sp:varName "kpi" ;
                          ] ;
                      ] ;
                    sp:arg3 "_" ;
                    sp:arg4 [
                        rdf:type afn:localname ;
                        sp:arg1 [
                            sp:varName "organisationalUnit" ;
                          ] ;
                      ] ;
                    sp:arg5 "_value" ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "valueInstance" ;
              ] ;
          ]
        ) ;
      rdfs:comment "4 Calculate score of organisational units based on minimum score of individuals assigned to organisational unit" ;
    ] ;
  spin:rule [
      rdf:type sp:Construct ;
      sp:templates (
