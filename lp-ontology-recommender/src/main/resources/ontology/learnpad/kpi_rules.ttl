  spin:rule [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object [
                sp:varName "score" ;
              ] ;
            sp:predicate <http://learnpad.eu#performanceValueHasScore> ;
            sp:subject [
                sp:varName "kpiValue" ;
              ] ;
          ]
        ) ;
      sp:where (
          [
            rdf:type sp:SubQuery ;
            sp:query [
                rdf:type sp:Select ;
                sp:resultVariables (
                    [
                      sp:varName "kpiValue" ;
                    ]
                    [
                      sp:varName "actualValue" ;
                    ]
                    [
                      sp:varName "score" ;
                    ]
                  ) ;
                sp:where (
                    [
                      rdf:type sp:TriplePath ;
                      sp:object spin:_this ;
                      sp:path [
                          rdf:type sp:ModPath ;
                          sp:modMax -2 ;
                          sp:modMin 0 ;
                          sp:subPath rdfs:subClassOf ;
                        ] ;
                      sp:subject [
                          sp:varName "kpi" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "kpi" ;
                        ] ;
                      sp:predicate rdf:type ;
                      sp:subject [
                          sp:varName "kpiValue" ;
                        ] ;
                    ]
                    [
                      sp:object <http://learnpad.eu#PerformanceValue> ;
                      sp:predicate rdf:type ;
                      sp:subject [
                          sp:varName "kpiValue" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "actualValue" ;
                        ] ;
                      sp:predicate <http://learnpad.eu#performanceValueHasActualValue> ;
                      sp:subject [
                          sp:varName "kpiValue" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "greenYellowThreshold" ;
                        ] ;
                      sp:predicate <http://ikm-group.ch/archiMEO/kpi#kpiHasGreenYellowThreshold> ;
                      sp:subject [
                          sp:varName "kpi" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "yellowRedThreshold" ;
                        ] ;
                      sp:predicate <http://ikm-group.ch/archiMEO/kpi#kpiHasYellowRedThreshold> ;
                      sp:subject [
                          sp:varName "kpi" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "greaterIsBetter" ;
                        ] ;
                      sp:predicate <http://ikm-group.ch/archiMEO/kpi#kpiHasThresholdTypeGreaterIsBetter> ;
                      sp:subject [
                          sp:varName "kpi" ;
                        ] ;
                    ]
                    [
                      rdf:type sp:Bind ;
                      sp:expression [
                          rdf:type sp:if ;
                          sp:arg1 [
                              sp:varName "greaterIsBetter" ;
                            ] ;
                          sp:arg2 [
                              rdf:type sp:if ;
                              sp:arg1 [
                                  rdf:type sp:ge ;
                                  sp:arg1 [
                                      sp:varName "actualValue" ;
                                    ] ;
                                  sp:arg2 [
                                      sp:varName "greenYellowThreshold" ;
                                    ] ;
                                ] ;
                              sp:arg2 3 ;
                              sp:arg3 [
                                  rdf:type sp:if ;
                                  sp:arg1 [
                                      rdf:type sp:ge ;
                                      sp:arg1 [
                                          sp:varName "actualValue" ;
                                        ] ;
                                      sp:arg2 [
                                          sp:varName "yellowRedThreshold" ;
                                        ] ;
                                    ] ;
                                  sp:arg2 2 ;
                                  sp:arg3 1 ;
                                ] ;
                            ] ;
                          sp:arg3 [
                              rdf:type sp:if ;
                              sp:arg1 [
                                  rdf:type sp:le ;
                                  sp:arg1 [
                                      sp:varName "actualValue" ;
                                    ] ;
                                  sp:arg2 [
                                      sp:varName "greenYellowThreshold" ;
                                    ] ;
                                ] ;
                              sp:arg2 3 ;
