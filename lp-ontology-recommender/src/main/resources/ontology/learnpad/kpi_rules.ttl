                        ] ;
                      sp:predicate <http://learnpad.eu#simulationScoreOfPerformer> ;
                      sp:subject [
                          sp:varName "bpSimulationScore" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "score" ;
                        ] ;
                      sp:predicate <http://learnpad.eu#hasSimulationScore> ;
                      sp:subject [
                          sp:varName "bpSimulationScore" ;
                        ] ;
                    ]
                  ) ;
              ] ;
          ]
          [
            rdf:type sp:SubQuery ;
            sp:query [
                rdf:type sp:Select ;
                sp:resultVariables (
                    [
                      sp:expression [
                          rdf:type sp:Max ;
                          sp:expression [
                              sp:varName "timestamp" ;
                            ] ;
                        ] ;
                      sp:varName "latestTimestamp" ;
                    ]
                  ) ;
                sp:where (
                    [
                      sp:object <http://learnpad.eu#BPSimulationScore> ;
                      sp:predicate rdf:type ;
                      sp:subject [
                          sp:varName "bpSimulationScore" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "person" ;
                        ] ;
                      sp:predicate <http://learnpad.eu#simulationScoreOfPerformer> ;
                      sp:subject [
                          sp:varName "bpSimulationScore" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "score" ;
                        ] ;
                      sp:predicate <http://learnpad.eu#hasSimulationScore> ;
                      sp:subject [
                          sp:varName "bpSimulationScore" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "timestamp" ;
                        ] ;
                      sp:predicate <http://learnpad.eu#simulationScoreCreatedAt> ;
                      sp:subject [
                          sp:varName "bpSimulationScore" ;
                        ] ;
                    ]
                  ) ;
              ] ;
          ]
          [
            sp:object <http://ikm-group.ch/archiMEO/kpi#KPI> ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "kpi" ;
              ] ;
          ]
          [
            sp:object "total no of simulations" ;
            sp:predicate rdfs:label ;
            sp:subject [
                sp:varName "kpi" ;
              ] ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type sp:iri ;
                sp:arg1 [
                    rdf:type fn:concat ;
                    sp:arg1 "http://learnpad.eu/exec#" ;
                    sp:arg2 [
                        rdf:type afn:localname ;
                        sp:arg1 [
                            sp:varName "kpi" ;
                          ] ;
                      ] ;
                    sp:arg3 "_" ;
                    sp:arg4 [
                        rdf:type afn:localname ;
                        sp:arg1 [
                            sp:varName "person" ;
                          ] ;
                      ] ;
                    sp:arg5 "_" ;
                    sp:arg6 [
                        sp:varName "latestTimestamp" ;
                      ] ;
                    sp:arg7 "_value" ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "valueInstance" ;
              ] ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type sp:now ;
              ] ;
            sp:variable [
                sp:varName "creationTimestamp" ;
              ] ;
          ]
          [
            rdf:type sp:Filter ;
            sp:expression [
                rdf:type sp:notExists ;
                sp:elements (
                    [
                      sp:object <http://learnpad.eu#PerformanceValue> ;
                      sp:predicate rdf:type ;
                      sp:subject [
                          sp:varName "valueInstance" ;
                        ] ;
                    ]
                  ) ;
              ] ;
          ]
        ) ;
      rdfs:comment "2 Rule for: \"total no of simulations\"" ;
    ] ;
