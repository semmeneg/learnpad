                sp:where (
                    [
                      sp:object <http://learnpad.eu#BPSimulationScore> ;
                      sp:predicate rdf:type ;
                      sp:subject [
                          sp:varName "bpSimulationScore" ;
                        ] ;
                    ]
                    [
                      sp:object [
