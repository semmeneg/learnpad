      sp:where (
          [
            rdf:type sp:SubQuery ;
            sp:query [
                rdf:type sp:Select ;
                sp:groupBy (
                    [
                      sp:varName "person" ;
                    ]
                  ) ;
                sp:resultVariables (
                    [
